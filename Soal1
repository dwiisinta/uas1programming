data = [["42130105", "Ni Kadek Dwi Sinta Mahayani", "Teknologi Informasi"], ["82122466", "I Nyoman Aryadhi Suartana", "Ilmu Hukum"],
        ["42130108", "Shane Johanes Welan", "Teknologi Informasi"]]
x = 0
print("Silahkan Pilih nama mahasiswa berikut : ")
for i in data:
    print(f"{x+1}. {data[x][1]}")
    x = x + 1

user = int(input("Pilihan anda : "))

if user == 1:
    print(data[0])
elif user == 2:
    print(data[1])
elif user == 3:
    print(data[2])

